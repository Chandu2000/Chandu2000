#include <stdio.h>

int main() {

    int a,b,c;

    printf(“Enter the values of a and b and c:\n”);

    scanf(“%d %d %d”,&a,&b,&c);

    if(a>b && a>c){

        printf(“%d is the greatest number”,a);

    }

    else if(b>c){

        printf(“%d is the greatest number”,b);

    }

    else{

        printf(“%d is the greatest number”,c);

    }

    return 0;

}

